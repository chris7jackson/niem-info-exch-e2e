<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:cb_ext="http://cb.acf.hhs.gov/neice/2.0/ext/"
	xmlns:cb_code="http://cb.acf.hhs.gov/neice/2.0/codes/"
	xmlns:i="http://release.niem.gov/niem/appinfo/4.0/"
	xmlns:s="http://release.niem.gov/niem/structures/4.0/"
	xmlns:j="http://release.niem.gov/niem/domains/jxdm/6.1/"
	xmlns:scr="http://release.niem.gov/niem/domains/screening/4.1/"
	xmlns:hs="http://release.niem.gov/niem/domains/humanServices/4.1/"
	xmlns:niem-xsd="http://release.niem.gov/niem/proxy/xsd/4.0/"
	xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
	xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="3"
	ct:conformanceTargets="http://reference.niem.gov/niem/specification/naming-and-design-rules/4.0/#ExtensionSchemaDocument"
	targetNamespace="http://cb.acf.hhs.gov/neice/2.0/ext/" elementFormDefault="qualified"
	blockDefault="extension">
	<xs:annotation>
		<xs:documentation>NEICE Extension Schema.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://cb.acf.hhs.gov/neice/2.0/codes/" schemaLocation="./neice_codes.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/appinfo/4.0/"
		schemaLocation="../niem/utility/appinfo/4.0/appinfo.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/conformanceTargets/3.0/"
		schemaLocation="../niem/utility/conformanceTargets/3.0/conformanceTargets.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/humanServices/4.1/"
		schemaLocation="../niem/domains/humanServices/4.1/hs.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/jxdm/6.1/"
		schemaLocation="../niem/domains/jxdm/6.1/jxdm.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/screening/4.1/"
		schemaLocation="../niem/domains/screening/4.1/screening.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/niem-core/4.0/"
		schemaLocation="../niem/niem-core/4.0/niem-core.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/proxy/xsd/4.0/"
		schemaLocation="../niem/proxy/xsd/4.0/xs.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/structures/4.0/"
		schemaLocation="../niem/utility/structures/4.0/structures.xsd"/>
	<xs:complexType name="NEICEDocumentAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about NEICE
				Documents</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:NEICEDocumentCode"/>
					<xs:element ref="cb_ext:NEICEDocumentCodeOtherText" minOccurs="0"/>
					<xs:element ref="hs:Child" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AdditionalElementType">
		<xs:annotation>
			<xs:documentation>A data type for a key/value pair for data needed, but not presently
				defined in the exchange.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:ElementKeyText" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="cb_ext:ElementValueText" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CaseStatusUpdateType">
		<xs:annotation>
			<xs:documentation>A data type for a case status update.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:CaseStatusUpdateCode" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:CaseStatusUpdateCodeOtherText" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:StatusUpdateEventDate" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CaseViolationType">
		<xs:annotation>
			<xs:documentation>A data type for details of a case violation.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:ActivityType">
				<xs:sequence>
					<xs:element ref="cb_ext:ViolationStateCode" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:ViolationCategoryCode" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ChildAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about hs's Child information
				with ICPC-related data elements</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:ChildCurrentLegalPlacementStatusCode" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:ChildCurrentLegalPlacementStatusOtherText" minOccurs="0"
						maxOccurs="1"/>
					<xs:element ref="cb_ext:ChildICWAEligibilityCode" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="cb_ext:ChildIVEDeterminationStatusCode" minOccurs="0"/>
					<xs:element ref="cb_ext:ChildNEICEIdentification" minOccurs="0"/>
					<xs:element ref="cb_ext:ChildStateIdentification" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CourtOrderType">
		<xs:annotation>
			<xs:documentation>A data type for additional details about a court
				order.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:DocumentType">
				<xs:sequence>
					<xs:element ref="cb_ext:CourtOrderComplianceDate" minOccurs="0"/>
					<xs:element ref="cb_ext:CourtOrderEnteredDate" minOccurs="0"/>
					<xs:element ref="cb_ext:CourtOrderReceivedDate" minOccurs="0"/>
					<xs:element ref="cb_ext:CourtOrderRenderedDate" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DispositionAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about the disposition
				information with the role of the entity providing the disposition</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:DispositionEntityRoleText" minOccurs="1"/>
					<xs:element ref="cb_ext:DispositionCode" minOccurs="0"/>
					<xs:element ref="cb_ext:DispositionReasonCode" minOccurs="0"/>
					<xs:element ref="cb_ext:DispositionReasonCodeOtherText" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EntityAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about an EntityType with
				ICPC-related data elements</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:PlacementResourceStateIdentification" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:PlacementResourceID" minOccurs="0" maxOccurs="unbounded"
					/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HomeStudyRequestType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information needed
				for an interstate home study request</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:HomeStudyRequestingAgency" minOccurs="0"/>
					<xs:element ref="nc:ContactInformation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEChildInformation" maxOccurs="unbounded"/>
					<xs:element ref="nc:Person" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100a" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100b" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:RequestDueDate" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HomeStudyResponseType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information
				contained in an interstate home study response</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:NEICEChildInformation" minOccurs="1"
						maxOccurs="unbounded"/>
					<xs:element ref="nc:Person" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100aSigned" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FollowupRequestType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information needed
				for an interstate follow-up request</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="hs:Child" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100a" minOccurs="0"/>
					<xs:element ref="cb_ext:LastRequestSentDate" minOccurs="0"/>
					<xs:element ref="cb_ext:LastReportReceivedDate" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FollowupReportType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information returned
				in an interstate follow-up report, this typically is in response to a follow-up
				request</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="hs:Child" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100a" minOccurs="0"/>
					<xs:element ref="cb_ext:FollowupReportSupportingDocument" minOccurs="1"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
