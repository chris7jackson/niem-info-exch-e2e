		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates a frequently combined
				set of data structures, viz., child (one or more), parent (zero or more), and their
				relationships</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="hs:PlacementType">
				<xs:sequence>
					<xs:element ref="cb_ext:RelativePlacementResourcesCode" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType> -->
	<xs:complexType name="NEICEChildInformationType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates a frequently combined
				set of data structures, viz., child (one or more), parent (zero or more), and their
				relationships</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="hs:Child"/>
					<xs:element ref="hs:Parent" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="hs:ParentChildAssociation" minOccurs="0" maxOccurs="unbounded"
					/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NEICEDocument100aType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information in a
				100A document</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:DocumentType">
				<xs:sequence>
					<xs:element ref="hs:Child" maxOccurs="1"/>
					<xs:element ref="cb_ext:HomeStudyCategoryCode" minOccurs="0"/>
					<xs:element ref="cb_ext:IntendedPlacement" minOccurs="0"/>
					<xs:element ref="cb_ext:IntendedPlacementPlanningResponsibleParty" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:IntendedPlacementFinanceResponsibleParty" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SupervisingServicesRequestCode" minOccurs="0"
						maxOccurs="1"/>
					<xs:element ref="cb_ext:SupervisingServicesRequestOtherText" minOccurs="0"
						maxOccurs="1"/>
					<xs:element ref="cb_ext:ProposedSupervisoryAgency" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementResource"/>
					<xs:element ref="cb_ext:PlacementResourceRelatedPersonAssociation" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SupervisoryReportPeriodicityCode" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SupervisoryReportPeriodicityOtherText" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICE100aChildDocumentAttachment" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SendingAgencySignatory" minOccurs="0"/>
					<xs:element ref="cb_ext:SendingAgencySignatorySignedDate" minOccurs="0"/>
					<xs:element ref="cb_ext:SendingStateCompactSignatory" minOccurs="0"/>
					<xs:element ref="cb_ext:SendingStateSignatorySignedDate" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementRequestDisposition" minOccurs="0"/>
					<xs:element ref="cb_ext:InitialReportRequestedCode" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementPublicPrivateCategoryCode" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NEICEDocument100bType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the information in a
				100B document</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:DocumentType">
				<xs:sequence>
					<xs:element ref="hs:Child"/>
					<xs:element ref="cb_ext:PlacementResource" minOccurs="0"/>
					<xs:element ref="cb_ext:InitialPlacement" minOccurs="0"/>
					<xs:element ref="cb_ext:ChangePlacement" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementTermination" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100bAttachments" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SendingAgencySignatory" minOccurs="0"/>
					<xs:element ref="cb_ext:SendingAgencySignatorySignedDate" minOccurs="0"/>
					<xs:element ref="cb_ext:SendingStateCompactSignatory"/>
					<xs:element ref="cb_ext:SendingStateSignatorySignedDate"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CustodyAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about
				hs:ChildLegalCustodyPartyAssociationType with ICPC-related
				information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:RelativeCustodyRelationshipText" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:OtherCustodyDescriptionText" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:OtherCustodyRelationshipText" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlacementAugmentationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about hs:Placement data type
				with ICPC related data elements</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:AugmentationType">
				<xs:sequence>
					<xs:element ref="cb_ext:PlacementUnderEmergencyConditionsIndicator"
						minOccurs="0" maxOccurs="1"/>
					<xs:element ref="cb_ext:PlacementByPriorityIndicator" minOccurs="0"/>
					<xs:element ref="cb_ext:SpecialRequestText" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:PlacementAdoptionIndicator" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementIVESubsidyCode" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementAdoptionFinalizationLocationCode" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementTerminatedIndicator" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementCategoryCodeOtherText" minOccurs="0"/>
					<xs:element ref="cb_ext:ProspectiveAdoptiveFosterResource" minOccurs="0"/>
					<xs:element ref="cb_ext:RelativePlacementResourcesCode" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlacementDecisionType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that describes a placement decision in an
				ICPC case</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:NEICEChildInformation" minOccurs="1"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:NEICEDocument100b" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlacementResourceRelatedPersonAssociationType">
		<xs:annotation>
			<xs:documentation>A data type for an association between a placement resource and a
				person</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:PersonAssociationType">
				<xs:sequence>
					<xs:element ref="cb_ext:PlacementResourceRelatedPersonCategoryCode"
						minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlacementTerminationSupportDocumentationType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that encapsulates the documentation needed
				when a placement is terminated</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:AdoptionFinalizationCourtOrder" minOccurs="0"/>
					<xs:element ref="cb_ext:ParentalCustodyCourtOrder" minOccurs="0"/>
					<xs:element ref="cb_ext:RelativeCustodyCourtOrder" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlacementTerminationType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that describes a placement
				termination</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:ActivityType">
				<xs:sequence>
					<xs:element ref="cb_ext:PlacementTerminationReasonCode"/>
					<xs:element ref="cb_ext:CourtOrderAttachmentIndicator" minOccurs="0"/>
					<xs:element ref="cb_ext:UnusedPlacementResource" minOccurs="0"/>
					<xs:element ref="cb_ext:WithdrawnPlacementResource" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementTerminationReasonOtherText" minOccurs="0"/>
					<xs:element ref="hs:CustodialEntity" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="hs:ChildLegalCustodyPartyAssociation" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementTerminationSupportDocumentation" minOccurs="0"
					/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProvisionalApprovalType">
		<xs:annotation>
			<xs:documentation>A data type for a provisional approval.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:ProvisionalApprovalIndicator"/>
					<xs:element ref="hs:Child"/>
					<xs:element ref="nc:Date"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProvisionalDecisionType">
		<xs:annotation>
			<xs:documentation>A data type for a provisional decision.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:choice>
					<xs:element ref="cb_ext:ProvisionalApproval"/>
					<xs:element ref="cb_ext:ProvisionalDenial"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProvisionalDenialType">
		<xs:annotation>
			<xs:documentation>A data type for a provisional denial.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="cb_ext:ProvisionalDenialIndicator"/>
					<xs:element ref="hs:Child"/>
					<xs:element ref="nc:Date"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RelatedCaseType">
		<xs:annotation>
			<xs:documentation>A data type for a case which is related in some way to
				another.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="hs:RelatedCaseType">
				<xs:sequence>
					<xs:element ref="cb_ext:CaseNEICEIdentification" minOccurs="1"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:SendingStateCaseIdentification" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:ReceivingStateCaseIdentification" minOccurs="0"
						maxOccurs="unbounded"/>
					<xs:element ref="cb_ext:CaseRelationshipDescriptionText" minOccurs="0"
						maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResourceDetailType">
		<xs:annotation>
			<xs:documentation>A data type for a structure that describes the details of a
				Resource</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="s:ObjectType">
				<xs:sequence>
					<xs:element ref="nc:Entity"/>
					<xs:element ref="nc:Address" minOccurs="0"/>
					<xs:element ref="nc:ContactInformation" minOccurs="0"/>
					<xs:element ref="cb_ext:ResourceStateIdentification" minOccurs="0"/>
					<xs:element ref="cb_ext:PlacementCategoryCode" minOccurs="0" />
					<xs:element ref="cb_ext:PlacementCategoryCodeOtherText" minOccurs="0" />
