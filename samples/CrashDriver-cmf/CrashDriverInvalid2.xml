<?xml version="1.0" encoding="UTF-8"?>
<exch:CrashDriverInfo 
  xmlns:exch="http://example.com/CrashDriver/1.2/"
  xmlns:hs="https://docs.oasis-open.org/niemopen/ns/model/domains/humanServices/6.0/"
  xmlns:j="https://docs.oasis-open.org/niemopen/ns/model/domains/justice/6.0/"
  xmlns:nc="https://docs.oasis-open.org/niemopen/ns/model/niem-core/6.0/"
  xmlns:priv="http://example.com/PrivacyMetadata/2.0/"
  xmlns:unece="https://docs.oasis-open.org/niemopen/ns/model/codes/unece_rec20/6.0/"
  xmlns:structures="https://docs.oasis-open.org/niemopen/ns/model/structures/6.0/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <!-- INVALID: Missing required j:Crash element (required by XSD) -->
  <!-- INVALID: j:Crash element is required by CrashDriverInfoType but missing -->

  <!-- INVALID: Invalid element not defined in schema -->
  <exch:InvalidElement>This element should not exist</exch:InvalidElement>

  <!-- INVALID: Wrong namespace for Date element -->
  <j:Charge>
    <!-- INVALID: Wrong data type - date should be in proper format -->
    <j:ChargeDate>
      <nc:Date>not-a-valid-date-format</nc:Date>
    </j:ChargeDate>
    
    <!-- INVALID: Missing required ChargeDescriptionText -->
    <!-- j:ChargeDescriptionText is required but missing -->
    
    <!-- INVALID: Wrong data type - boolean expected -->
    <j:ChargeFelonyIndicator>maybe</j:ChargeFelonyIndicator>
    
    <!-- INVALID: Extra element not in schema -->
    <j:InvalidChargeElement>Should not exist</j:InvalidChargeElement>
  </j:Charge>

  <!-- INVALID: PersonChargeAssociation without proper references -->
  <j:PersonChargeAssociation>
    <!-- INVALID: Missing required Person element -->
    <!-- INVALID: Missing required Charge element -->
    <j:JuvenileAsAdultIndicator>not-a-boolean</j:JuvenileAsAdultIndicator>
  </j:PersonChargeAssociation>

  <!-- INVALID: Metadata with invalid structure -->
  <nc:Metadata structures:id="INVALID_METADATA">
    <!-- INVALID: Wrong element in MetadataAugmentation -->
    <j:MetadataAugmentation>
      <j:InvalidMetadataElement>Should not exist</j:InvalidMetadataElement>
    </j:MetadataAugmentation>
  </nc:Metadata>

  <!-- INVALID: PrivacyMetadata without proper structure -->
  <priv:PrivacyMetadata>
    <!-- INVALID: Missing structures:id attribute which is typically required -->
    <priv:InvalidPrivacyElement>Invalid</priv:InvalidPrivacyElement>
  </priv:PrivacyMetadata>

  <!-- INVALID: Element in wrong order (violates sequence constraint) -->
  <!-- This would normally come before Metadata but appears after -->
  <j:Crash>
    <!-- INVALID: Missing required ActivityDate -->
    <!-- INVALID: Invalid vehicle structure -->
    <j:CrashVehicle>
      <j:CrashDriver>
        <!-- INVALID: Invalid date format -->
        <nc:PersonBirthDate>
          <nc:Date>1890-13-45</nc:Date> <!-- Invalid month and day -->
        </nc:PersonBirthDate>
        
      </j:CrashDriver>
    </j:CrashVehicle>
  </j:Crash>

</exch:CrashDriverInfo>