    "hs:PersonOtherKinAssociationCategoryCode": {
      "description": "A code list that describes the nature of the kinship or other close personal relationship from a source person to a target person.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/hs:PersonOtherKinAssociationCodeType"
      }
    },
    "nc:PersonName": {
      "description": "A combination of names and/or titles by which a person is known.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/nc:PersonNameType"
      }
    },
    "nc:ReportedDate": {
      "description": "A date information was observed, measured, identified, or became known.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/nc:DateType"
      }
    },
    "nc:Location2DGeospatialCoordinate": {
      "description": "A location identified by a latitude and longitude.",
      "type": "array",
      "items": {
        "$ref": "#/definitions/nc:Location2DGeospatialCoordinateType"
      }
    }
  },
  "definitions": {
    "aamva_d20:AccidentSeverityCodeType": {
      "pattern": "^\\S*$",
      "description": "A data type for severity levels of an accident.",
      "type": "string",
      "oneOf": [
        {
          "const": "1",
          "description": "Fatal Accident"
        },
        {
          "const": "2",
          "description": "Incapacitating Injury Accident"
        },
        {
          "const": "3",
          "description": "Non-incapacitating Evident Injury"
        },
        {
          "const": "4",
          "description": "Possible Injury Accident"
        },
        {
          "const": "5",
          "description": "Non-injury Accident"
        },
        {
          "const": "9",
          "description": "Unknown"
        }
      ]
    },
